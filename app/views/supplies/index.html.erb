
<h2>My Supplies</h2>
  <div class="btn-group" role="group" aria-label="Basic example">
    <%= button_to "Add a Supply", new_supply_path(current_user), :method=> "get", class: "btn btn-secondary btn-sm" %>&nbsp;
  </div><br><br>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>Supply Name</th>
            </tr>
          </thead>
          <tbody>
            <tr>
            <%@supplies.each do |supply|%>
              <td><%= link_to supply.supply_name, supply_path(supply) %></td>
            </tr>
            <%end%>
          </tbody>
        </table>
      </div>
    </main>
  </div>
</div>
